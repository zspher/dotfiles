[bismuth geometery]
Description=bismuth geometery size
ignoregeometry=true
ignoregeometryrule=2
minsize=1,1
minsizerule=2
placement=1
placementrule=2
types=1
wmclass=^(?!krunner).*$
wmclassmatch=3

[force system border]
Description=use system border
noborderrule=2
types=1
wmclass=^(resolve|steam).*$
wmclassmatch=3
